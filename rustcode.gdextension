[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rustcode/target/debug/librustcode.so"
linux.release.x86_64 =   "res://rustcode/target/release/librustcode.so"
windows.debug.x86_64 =   "res://rustcode/target/debug/rustcode.dll"
windows.release.x86_64 = "res://rustcode/target/release/rustcode.dll"
macos.debug =            "res://rustcode/target/debug/librustcode.dylib"
macos.release =          "res://rustcode/target/release/librustcode.dylib"
macos.debug.arm64 =      "res://rustcode/target/debug/librustcode.dylib"
macos.release.arm64 =    "res://rustcode/target/release/librustcode.dylib"
